					<control type="label">
						<left>20</left>
						<right>20</right>
						<top>0</top>
						<bottom>0</bottom>
						<align>right</align>
						<aligny>center</aligny>
						<font>$PARAM[font]</font>
						<label>$VAR[ListLabel2Var]</label>
						<shadowcolor>text_shadow</shadowcolor>
