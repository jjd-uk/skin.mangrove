						<left>20</left>
						<centertop>50%</centertop>
						<width>30</width>
						<height>30</height>
						<aligny>center</aligny>
						<texture>$VAR[AddonsListIconVar]</texture>
					</control>
					<control type="label">
						<left>70</left>
						<right>40</right>
						<aligny>center</aligny>
						<font>font28</font>
						<label>$INFO[ListItem.Label]</label>
					</control>
					<control type="label">
						<left>70</left>
						<right>70</right>
						<align>right</align>
						<aligny>center</aligny>
						<font>font28</font>
						<label>$VAR[AddonsLabel2Var]</label>
						<textcolor>grey</textcolor>
						<shadowcolor>text_shadow</shadowcolor>
					</control>
					<control type="image">
						<right>20</right>
						<centertop>50%</centertop>
						<width>32</width>
						<height>32</height>
						<texture>$VAR[AddonsOriginVar]</texture>
						<visible>!ListItem.IsFolder</visible>
					</control>
				</itemlayout>
