						<left>0</left>
						<right>0</right>
						<top>0</top>
						<bottom>0</bottom>
						<texture colordiffuse="panel_tint">colors/white.png</texture>
						<animation effect="fade" start="100" end="50" time="100" condition="!Control.HasFocus($PARAM[list_id])">Conditional</animation>
					</control>
					<control type="image">
						<left>20</left>
						<centertop>50%</centertop>
						<width>30</width>
						<height>30</height>
						<texture colordiffuse="blue">$VAR[AddonsListIconVar]</texture>
					</control>
					<control type="label">
						<left>75</left>
						<height>60</height>
						<right>40</right>
						<aligny>center</aligny>
						<scroll>true</scroll>
						<font>font28</font>
						<textcolor>blue</textcolor>
						<label>$INFO[ListItem.Label]</label>
					</control>
					<control type="label">
						<left>75</left>
						<right>70</right>
						<align>right</align>
						<aligny>center</aligny>
						<font>font28</font>
						<textcolor>blue</textcolor>
						<label>$VAR[AddonsLabel2Var]</label>
						<scroll>true</scroll>
					</control>
					<control type="image">
						<right>20</right>
						<centertop>50%</centertop>
						<width>30</width>
						<height>30</height>
						<texture colordiffuse="blue">$VAR[AddonsOriginVar]</texture>
						<visible>!ListItem.IsFolder</visible>
					</control>
				</focusedlayout>
				<itemlayout height="list_item_height" condition="Container.Content(addons) | Container.Content()">
